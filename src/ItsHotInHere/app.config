<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="ItsHotInHere.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <system.diagnostics>
        <sources>
            <source name="Tesseract" switchValue="Verbose">
                <listeners>
                    <clear/>
                    <add name="console"/>
                    <!--<add name="file" />-->
                </listeners>
            </source>
        </sources>
        <sharedListeners>
            <add name="console" type="System.Diagnostics.ConsoleTraceListener"/>
        </sharedListeners>
    </system.diagnostics>
    <userSettings>
        <ItsHotInHere.Settings>
            <setting name="threshold" serializeAs="String">
                <value>3</value>
            </setting>
            <setting name="erode" serializeAs="String">
                <value>1</value>
            </setting>
            <setting name="psm" serializeAs="String">
                <value>11</value>
            </setting>
            <setting name="gray" serializeAs="String">
                <value>10</value>
            </setting>
        </ItsHotInHere.Settings>
    </userSettings>
</configuration>
